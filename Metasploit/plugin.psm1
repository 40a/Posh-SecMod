        Position=1)]
        [string]$Name,

        # Plugin Options
        [Parameter(Mandatory=$false,
        ParameterSetName = "Session",
        Position=2)]
        [Parameter(Mandatory=$false,
        ParameterSetName = "Index",
        Position=2)]
        [hashtable]$Options = @{}
    )
    BEGIN 
    {
        
    }
    PROCESS 
    {    
        if ($Id -ge 0)
        {
            foreach($conn in $Global:MetasploitConn)
